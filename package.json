{
  "name": "whatsroom-frontend",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "start:go": "expo start --lan -c --port 8081",
    "start:tunnel": "expo start --tunnel -c --port 8081",
    "usb:prep": "adb reverse tcp:8081 tcp:8081 && adb reverse tcp:4000 tcp:4000",
    "start:usb": "npm run usb:prep && expo start --localhost -c --port 8081",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "backend": "npm --prefix backend run dev",
    "backend:start": "npm --prefix backend run start"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "axios": "^1.11.0",
    "expo": "~54.0.33",
    "expo-av": "~16.0.8",
    "expo-clipboard": "~8.0.8",
    "expo-constants": "~18.0.13",
    "expo-font": "~14.0.11",
    "expo-linear-gradient": "~15.0.2",
    "expo-linking": "~8.0.11",
    "expo-router": "~6.0.23",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.5.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@expo/ngrok": "^4.1.3",
    "babel-preset-expo": "~54.0.10"
  }
}
